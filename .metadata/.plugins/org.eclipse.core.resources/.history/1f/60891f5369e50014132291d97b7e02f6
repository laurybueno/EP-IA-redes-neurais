
public class LVQ {
	public static void main (String []args){
		//R = numero de atributos da base de dados que serao analisados (64)
		Input objeto = new Input();
		String teste = objeto.leitura("optdigitsNormalizado.txt", 1);
		double[] atributos = objeto.StringToDouble(teste);
		for(int i=0;i<atributos.length;i++)
			System.out.println(atributos[i]);
	}
}
